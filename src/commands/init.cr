require "file_utils"

module Beulogue
  module Commands
    class Init
      def initialize(args : Array(String))
        @name = if args[0]?
                  Path[args[0]].expand
                else
                  Path[Dir.current]
                end

        Log.debug { "Init in #{@name}" }

        create_contents
        create_assets
        create_templates
        create_config

        puts <<-DATA
        Your website is available in #{@name}!

        Run `beulogue` in #{@name} to generate your pages, and serve the "public" folder.

        Read more:
        => documentation: https://beulogue.ehret.me/
        => beulogue templates:Â https://github.com/SiegfriedEhret/beulogue-templates/
        DATA

        exit 0
      end

      private def create_contents
        f = @name / "content"
        FileUtils.mkdir_p f.to_s

        content_index = <<-DATA
        ---
        title: My title
        date: 2019-07-28
        description: A small description
        ---

        This content is from the `content/_index.md` file.

        DATA
        File.write(f / "_index.md", content_index)

        content_first_post = <<-DATA
        ---
        title: My awesome post
        date: 2019-07-28
        description: A small description
        ---

        Yoohoo ! It works !

        [![Jmills74 at English Wikipedia [CC BY-SA 3.0 (http://creativecommons.org/licenses/by-sa/3.0/)], via Wikimedia Commons](/images/Fujiview.jpg)](https://commons.wikimedia.org/wiki/File:Fujiview.jpg)

        DATA
        File.write(f / "my-awesome-post.md", content_first_post)
      end

      private def create_assets
        f = @name / "public" / "images"
        FileUtils.mkdir_p f.to_s

        img = ""
        File.write(f / "Fujiview.jpg", Base64.decode_string(img))
      end

      private def create_templates
        f = @name / "templates"
        FileUtils.mkdir_p f.to_s

        content_list = <<-DATA
        <!DOCTYPE html>
        <html lang="{{language}}">
          <head>
            <meta name="generator" content="beulogue" />
            <meta charset="utf-8" />
            <link
              rel="alternate"
              type="application/rss+xml"
              title="RSS"
              href="/feed.xml"
            />
            <meta name="viewport" content="width=device-width, initial-scale=1" />
            <title>
              {{site.title}}
            </title>

            <link rel="stylesheet" href="/styles.css" />
          </head>

          <body>
            <header>
              <h1>
                {{title}}
              </h1>

              {{content | escape}}
            </header>

            {% for page in pages %}
            <section>
              <a href="{{page.url}}">{{page.title}}</a>

              <div>{{page.dateFormatted}}</div>

              <p>{{page.description}}</p>
            </section>
            {% endfor %}
          </body>
        </html>

        DATA
        File.write(f / "list.html", content_list)

        content_page = <<-DATA
        <!DOCTYPE html>
        <html lang="{{language}}">
          <head>
            <meta name="generator" content="beulogue" />
            <meta charset="utf-8" />
            <link
              rel="alternate"
              type="application/rss+xml"
              title="RSS"
              href="/feed.xml"
            />
            <meta name="viewport" content="width=device-width, initial-scale=1" />
            <title>
              {{ site.title }}
            </title>

            <link rel="stylesheet" href="/styles.css" />
          </head>

          <body>
            <h1>
              {{ title }}
            </h1>

            {{content | escape}}
          </body>
        </html>

        DATA
        File.write(f / "page.html", content_page)
      end

      private def create_config
        content = <<-DATA
        base: https://example.com
        title: My awesome website
        languages:
        - en

        # See https://beulogue.ehret.me/en/usage/configuration.html

        DATA

        filename = @name / "beulogue.yml"
        File.write(filename.to_s, content)
      end
    end
  end
end
